Cell[CellGroupData[{

Cell["Animations in Mathematica", "Section",
 CellChangeTimes->{{3.7503529909825954`*^9, 
  3.7503529937120695`*^9}},ExpressionUUID->"c8d15359-2cf5-495e-98a5-\
911845afb1f6"],

Cell[TextData[{
 "Recall that example trajectory/parameterization is\n\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     OverscriptBox["r", "_"], "(", "t", ")"}], "=", 
    RowBox[{
     RowBox[{"(", GridBox[{
        {
         RowBox[{"x", "(", "t", ")"}]},
        {
         RowBox[{"y", "(", "t", ")"}]},
        {
         RowBox[{"z", "(", "t", ")"}]}
       }], ")"}], "=", 
     RowBox[{"(", GridBox[{
        {
         RowBox[{"5", " ", 
          RowBox[{"cos", "(", "t", ")"}]}]},
        {
         RowBox[{"2", " ", 
          RowBox[{"sin", "(", "t", ")"}]}]},
        {"t"}
       }], ")"}]}]}], TraditionalForm]],ExpressionUUID->
  "02b03092-78e0-4643-8179-5eef45607b65"],
 "\n\t\n\tfor ",
 Cell[BoxData[
  FormBox[
   RowBox[{"t", "\[Element]", 
    RowBox[{"[", 
     RowBox[{"0", ",", " ", 
      RowBox[{"2", " ", "\[Pi]"}]}], "]"}]}], TraditionalForm]],
  ExpressionUUID->"fbce6485-ae07-42c8-9c5e-43cf00b74deb"]
}], "Text",
 CellChangeTimes->{{3.750353064827817*^9, 
  3.750353100334958*^9}},ExpressionUUID->"d2930d0b-b912-46f5-be1e-\
d2d0f4fc02d8"],

Cell[CellGroupData[{

Cell["Step 1:  Generate Data", "Subsection",
 CellChangeTimes->{{3.750353000823506*^9, 
  3.7503530071023436`*^9}},ExpressionUUID->"6e8b7565-e0a8-42f4-872c-\
19ae01f530aa"],

Cell["Define the aforementioned parameterization", "Text",
 CellChangeTimes->{{3.750353172063381*^9, 
  3.750353176883048*^9}},ExpressionUUID->"408c8217-35c6-4e96-95cd-\
b6ff015d4ce3"],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"x", "[", "t_", "]"}], "=", 
   RowBox[{"5", " ", 
    RowBox[{"Cos", "[", "t", "]"}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"y", "[", "t_", "]"}], "=", 
   RowBox[{"2", " ", 
    RowBox[{"Sin", "[", "t", "]"}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"z", "[", "t_", "]"}], "=", "t"}], ";"}], "\[IndentingNewLine]", 
  "\[IndentingNewLine]", 
  RowBox[{"(*", 
   RowBox[{
   "Check", " ", "that", " ", "this", " ", "looks", " ", "reasonable", " ", 
    "with", " ", "a", " ", "static", " ", "plot"}], 
   "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{"ParametricPlot3D", "[", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{
     RowBox[{"x", "[", "t", "]"}], ",", 
     RowBox[{"y", "[", "t", "]"}], ",", 
     RowBox[{"z", "[", "t", "]"}]}], "}"}], ",", 
   RowBox[{"{", 
    RowBox[{"t", ",", "0", ",", 
     RowBox[{"2", " ", "\[Pi]"}]}], "}"}]}], "]"}]}], "Input",
 CellChangeTimes->{{3.750353177607279*^9, 
