Cell[TextData[{
 "Setup a range of times where we would like to animate or render the \
scenario ",
 Cell[BoxData[
  FormBox[
   RowBox[{"(", 
    RowBox[{"t", "\[Element]", 
     RowBox[{"[", 
      RowBox[{"0", ",", " ", 
       RowBox[{"2", "\[Pi]"}]}], "]"}]}], ")"}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "0c8e4fc3-2109-4e27-8342-7e0b5860b186"],
 "."
}], "Text",
 CellChangeTimes->{{3.750353249120438*^9, 
  3.750353266450629*^9}},ExpressionUUID->"82ab755d-7cc4-449c-b05f-\
5da952babda7"],

Cell[BoxData[
 RowBox[{
  RowBox[{"tRange", "=", 
   RowBox[{"Range", "[", 
    RowBox[{"0", ",", " ", 
     RowBox[{"2", " ", "\[Pi]"}], ",", 
     FractionBox[
      RowBox[{"2", " ", "\[Pi]"}], "99"]}], "]"}]}], ";", "   ", 
  RowBox[{"(*", 
   RowBox[{
    RowBox[{"Create", " ", "100", " ", "time", " ", "steps"}], ",", " ", 
    RowBox[{"saved", " ", "as", " ", "a", " ", "list"}]}], "*)"}]}]], "Input",
 CellChangeTimes->{{3.750353271106292*^9, 
  3.750353299364152*^9}},ExpressionUUID->"12ae23c1-1fe1-453f-8bfc-\
643242fd8ed2"],

Cell["\<\
Define/initialize a container that will be used to hold the frames of \
animation.\
\>", "Text",
 CellChangeTimes->{{3.7503533140623207`*^9, 
  3.750353324937551*^9}},ExpressionUUID->"2a87a96d-984d-4613-a30a-\
8400fb7eb53c"],

Cell[BoxData[
 RowBox[{
  RowBox[{"movieVector", "=", 
   RowBox[{"ConstantArray", "[", 
    RowBox[{"0", ",", 
     RowBox[{"Length", "[", "tRange", "]"}]}], "]"}]}], ";", "  ", 
  RowBox[{"(*", 
   RowBox[{"saved", " ", "as", " ", "a", " ", "list"}], "*)"}]}]], "Input",
 CellChangeTimes->{{3.750353333748355*^9, 
  3.750353361006672*^9}},ExpressionUUID->"bfbd54a1-9031-45f7-9c8b-\
c05c67608c6b"]
}, Open  ]],

Cell["Step 2:  Draw/Render the Scenario", "Subsection",
 CellChangeTimes->{{3.750353000823506*^9, 
  3.7503530205900126`*^9}},ExpressionUUID->"1a2dadbb-4408-47cc-b822-\
f63890bf1d55"],

Cell["Step 3:  Take a Snapshot", "Subsection",
 CellChangeTimes->{{3.750353000823506*^9, 
  3.7503530284775295`*^9}},ExpressionUUID->"f92f5763-c6f3-4b3a-b458-\
52db7934cdb0"],

Cell[CellGroupData[{

Cell["Step 4:  Advance Time Forward", "Subsection",
 CellChangeTimes->{{3.750353000823506*^9, 
  3.7503530368418007`*^9}},ExpressionUUID->"415666e3-015e-47a3-9625-\
ebc2879a6a2b"],

Cell["Use a \[OpenCurlyQuote]for\[CloseCurlyQuote] loop to create frames of \
animation", "Text",
 CellChangeTimes->{{3.750353451552348*^9, 
  3.750353457162631*^9}},ExpressionUUID->"077f21b6-817a-404e-bdaa-\
74f393cc8e07"],

Cell[BoxData[
 RowBox[{"For", "[", 
  RowBox[{
   RowBox[{"k", "=", "1"}], ",", 
   RowBox[{"k", "\[LessEqual]", 
    RowBox[{"Length", "[", "tRange", "]"}]}], ",", 
   RowBox[{"k", "++"}], ",", "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{
    "Extract", " ", "data", " ", "for", " ", "a", " ", "single", " ", 
     "point"}], "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"tk", "=", 
     RowBox[{"tRange", "[", 
      RowBox[{"[", "k", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
    RowBox[{"xk", "=", 
     RowBox[{"x", "[", "tk", "]"}]}], ";", "\[IndentingNewLine]", 
    RowBox[{"yk", "=", 
     RowBox[{"y", "[", "tk", "]"}]}], ";", "\[IndentingNewLine]", 
    RowBox[{"zk", "=", 
     RowBox[{"z", "[", "tk", "]"}]}], ";", "\[IndentingNewLine]", 
    "\[IndentingNewLine]", 
    RowBox[{"(*", 
     RowBox[{"Render", " ", "the", " ", "scenario"}], "*)"}], 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"movieVector", "[", 
      RowBox[{"[", "k", "]"}], "]"}], "=", 
     RowBox[{"Show", "[", "\[IndentingNewLine]", 
      RowBox[{"(*", 
       RowBox[{
       "Plotting", " ", "just", " ", "the", " ", "point", " ", "as", " ", "a",
         " ", "green", " ", "dot"}], "*)"}], "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"Graphics3D", "[", "\[IndentingNewLine]", 
        RowBox[{"{", "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{"AbsolutePointSize", "[", "15", "]"}], ",", "Green", ",", 
          " ", 
          RowBox[{"Point", "[", 
           RowBox[{"{", 
            RowBox[{"xk", ",", "yk", ",", "zk"}], "}"}], "]"}]}], 
         "\[IndentingNewLine]", "}"}], "\[IndentingNewLine]", "]"}], ",", 
       "\[IndentingNewLine]", "\[IndentingNewLine]", 
       RowBox[{"(*", 
        RowBox[{"draw", " ", "the", " ", "entire", " ", 
         RowBox[{"trajectory", "/", "curve"}]}], "*)"}], 
       "\[IndentingNewLine]", 
       RowBox[{"ParametricPlot3D", "[", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{
           RowBox[{"x", "[", "t", "]"}], ",", 
           RowBox[{"y", "[", "t", "]"}], ",", 
           RowBox[{"z", "[", "t", "]"}]}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"t", ",", "0", ",", 
           RowBox[{"2", " ", "\[Pi]"}]}], "}"}]}], "]"}], ",", 
       "\[IndentingNewLine]", "\[IndentingNewLine]", 
       RowBox[{"(*", 
        RowBox[{"Decorate", " ", "the", " ", "plot"}], "*)"}], 
       "\[IndentingNewLine]", 
       RowBox[{"AxesLabel", "\[Rule]", 
        RowBox[{"{", 
         RowBox[{"\"\<x\>\"", ",", "\"\<y\>\"", ",", "\"\<z\>\""}], "}"}]}], 
       ",", "\[IndentingNewLine]", 
       RowBox[{"PlotLabel", "\[Rule]", 
        RowBox[{"StringJoin", "[", 
         RowBox[{"\"\<t = \>\"", ",", " ", 
          RowBox[{"ToString", "[", 
           RowBox[{"N", "[", "tk", "]"}], "]"}]}], "]"}]}]}], 
      "\[IndentingNewLine]", "]"}]}]}]}], "\[IndentingNewLine]", 
  "]"}]], "Input",
 CellChangeTimes->{{3.7503534696122355`*^9, 3.750353686608765*^9}, {
  3.7503537190590553`*^9, 3.750353745836733*^9}, {3.7503537917754354`*^9, 
  3.7503538416261888`*^9}, {3.750354818748751*^9, 
  3.7503548291582603`*^9}},ExpressionUUID->"2d57bb8c-758a-4370-a6c0-\
c58984c169bc"]
}, Open  ]],

